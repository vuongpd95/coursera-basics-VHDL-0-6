/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
	Not(in = b, out = notb);
	Not(in = a, out = nota);
	And(a = a, b = notb, out = aornotb);
	And(a = nota, b = b, out = notaorb);
	Or(a = aornotb, b = notaorb, out = out);
}